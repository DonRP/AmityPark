msgstr "[_valerie_s_uniform_point_s_towards_s_angry_s_distraught_] {i}¡¡...!! {/i}"

# game/code/character_events/lily/lily_introduction.rpy:143 #-#-# translate crowdin lily_introduction_f9a48a29:
msgid "[_danny_s_towards_s_rise_s_talk_displeased_] Valerie? You work here?"
msgstr "[_danny_s_towards_s_rise_s_talk_displeased_] ¿Valerie? ¿Trabajas aquí?"

# game/code/character_events/lily/lily_introduction.rpy:147 #-#-# translate crowdin lily_introduction_e558535a:
msgid "[_valerie_s_uniform_point_s_towards_wide_s_angry_s_talk_distraught_] F-F-Fenton!! What the, why are you-"
msgstr "[_valerie_s_uniform_point_s_towards_wide_s_angry_s_talk_distraught_] ¡¡F-F-Fenton!! ¿Qué?, ¿por qué estás-"

# game/code/character_events/lily/lily_introduction.rpy:151 #-#-# translate crowdin lily_introduction_74c559a8:
msgid "[_lily_s_towards_s_rise_s_talk_displeased_] Your name is \"Fenton\"?"
msgstr "[_lily_s_towards_s_rise_s_talk_displeased_] ¿Tu nombre es \"Fenton\"?"

# game/code/character_events/lily/lily_introduction.rpy:155 #-#-# translate crowdin lily_introduction_bc87aa2b:
msgid "[_danny_s_towards_s_sad_s_talk_happy_] No, that's my last name, my name's Da-"
msgstr "[_danny_s_towards_s_sad_s_talk_happy_] No, ese es mi apellido, mi nombre Da-"

# game/code/character_events/lily/lily_introduction.rpy:159 #-#-# translate crowdin lily_introduction_eaaee54b:
msgid "[_valerie_s_uniform_up_s_towards_s_angry_s_talk_displeased_] {i}FENTON!!{/i}{p}What the heck are you doing here?"
msgstr "[_valerie_s_uniform_up_s_towards_s_angry_s_talk_displeased_] {i}¡¡FENTON!! {/i}{p}¿Qué diablos estás haciendo aquí?"

# game/code/character_events/lily/lily_introduction.rpy:163 #-#-# translate crowdin lily_introduction_ba05f8e7:
msgid "[_danny_s_towards_s_rise_s_talk_consternation_] ...in a fast food restaurant? Jeez Val, take a guess."
msgstr "[_danny_s_towards_s_rise_s_talk_consternation_] ... en un restaurante de comida rápida? Jeez Val, adivina."

# game/code/character_events/lily/lily_introduction.rpy:167 #-#-# translate crowdin lily_introduction_48fc2535:
msgid "[_valerie_s_uniform_point_s_towards_s_angry_s_talk_distraught_] Look, you didn't see {u}anything{/u}, Fenton! If you know what's good for you."
msgstr "[_valerie_s_uniform_point_s_towards_s_angry_s_talk_distraught_] Mira, tú no has visto {u}nada {/u},¡Fenton! Ya sabes que es lo mejor que puedes hacer."

# game/code/character_events/lily/lily_introduction.rpy:171 #-#-# translate crowdin lily_introduction_82417584:
msgid "[_lily_s_towards_s_rise_s_talk_displeased_] Valerie, why are you yelling?"
msgstr "[_lily_s_towards_s_rise_s_talk_displeased_] Valerie, ¿por qué estás gritando?"

# game/code/character_events/lily/lily_introduction.rpy:178 #-#-# translate crowdin lily_introduction_59c14058:
msgid "[_valerie_s_uniform_up_s_towards_s_rise_s_talk_displeased_] Lily, I don't want him around here?"
msgstr "[_valerie_s_uniform_up_s_towards_s_rise_s_talk_displeased_] Lily, no le quiero por aquí."

# game/code/character_events/lily/lily_introduction.rpy:182 #-#-# translate crowdin lily_introduction_5b384bca:
msgid "[_lily_s_towards_s_sad_s_talk_displeased_] Why not?"
msgstr "[_lily_s_towards_s_sad_s_talk_displeased_] ¿Por qué no?"

# game/code/character_events/lily/lily_introduction.rpy:186 #-#-# translate crowdin lily_introduction_ea4687c6:
msgid "[_valerie_s_uniform_s_away_s_angry_s_talk_displeased_] Cause the loser goes to my school!!"
msgstr "[_valerie_s_uniform_s_away_s_angry_s_talk_displeased_] ¡¡porque es un perdedor que va a mi escuela!!"

# game/code/character_events/lily/lily_introduction.rpy:190 #-#-# translate crowdin lily_introduction_e39d9c43:
msgid "[_lily_s_towards_s_idle_s_talk_happy_] You do?"
msgstr "[_lily_s_towards_s_idle_s_talk_happy_] ¿Lo eres?"

# game/code/character_events/lily/lily_introduction.rpy:194 #-#-# translate crowdin lily_introduction_815e3eea:
msgid "[_danny_s_towards_s_idle_s_talk_displeased_] Yeah..."
msgstr "[_danny_s_towards_s_idle_s_talk_displeased_] Sí..."

# game/code/character_events/lily/lily_introduction.rpy:198 #-#-# translate crowdin lily_introduction_037a86b7:
msgid "[_lily_s_towards_s_rise_s_talk_displeased_] Well, that's not really an excuse not to hire the guy..."
msgstr "[_lily_s_towards_s_rise_s_talk_displeased_] Bueno, eso no es una excusa para no contratarlo..."

# game/code/character_events/lily/lily_introduction.rpy:202 #-#-# translate crowdin lily_introduction_62b881c9:
msgid "[_valerie_s_uniform_point_s_towards_s_angry_s_talk_distraught_] Lilian, if you hire him, I quit!"
msgstr "[_valerie_s_uniform_point_s_towards_s_angry_s_talk_distraught_] Lilian, si lo contratas, ¡yo lo dejo!"

# game/code/character_events/lily/lily_introduction.rpy:207 #-#-# translate crowdin lily_introduction_123f09c9:
msgid "[_lily_s_towards_s_idle_s_talk_displeased_] Didn't you say you are in serious need of money since your Dad lost his job?"
msgstr "[_lily_s_towards_s_idle_s_talk_displeased_] ¿No dijiste que estás en necesidad de dinero desde que tu padre perdió su trabajo?"

# game/code/character_events/lily/lily_introduction.rpy:211 #-#-# translate crowdin lily_introduction_853e075e:
msgid "[_valerie_s_uniform_s_closed_s_sad_s_distraught_] {i}...!{/i}"
msgstr "[_valerie_s_uniform_s_closed_s_sad_s_distraught_] {i}¡...!{/i}"

# game/code/character_events/lily/lily_introduction.rpy:215 #-#-# translate crowdin lily_introduction_192c9958:
msgid "[_valerie_s_uniform_s_towards_s_sad_s_talk_distraught_] D-don't go around talking about that!"
msgstr "[_valerie_s_uniform_s_towards_s_sad_s_talk_distraught_] ¡No hables de esto a nadie!"

# game/code/character_events/lily/lily_introduction.rpy:219 #-#-# translate crowdin lily_introduction_417d49d4:
msgid "[_valerie_s_uniform_s_closed_s_angry_s_talk_displeased_] {i}Ugh!{/i} Fine! Do whatever you want."
msgstr "[_valerie_s_uniform_s_closed_s_angry_s_talk_displeased_] {i}Ugh! {/i} ¡Bien! Haz lo que quieras."

# game/code/character_events/lily/lily_introduction.rpy:228 #-#-# translate crowdin lily_introduction_dccc8885:
msgid "[_valerie_s_uniform_point_s_towards_s_angry_s_talk_displeased_] Just don't tell {u}anybody{/u} about this Fenton...{p}...or you're dead!"
msgstr "[_valerie_s_uniform_point_s_towards_s_angry_s_talk_displeased_] Simplemente no digas {u}a nadie {/u} sobre esto Fenton ...{p}... o ¡estás muerto!"

# game/code/character_events/lily/lily_introduction.rpy:232 #-#-# translate crowdin lily_introduction_29afd622:
msgid "[_danny_s_towards_s_rise_s_consternation_] ...?"
msgstr "[_danny_s_towards_s_rise_s_consternation_] ...?"

# game/code/character_events/lily/lily_introduction.rpy:241 #-#-# translate crowdin lily_introduction_53a4b846:
msgid "[_lily_s_towards_s_sad_s_talk_displeased_] ...{i}okaayyy{/i}... well I suppose I don't need to introduce you to Valerie..."
msgstr "[_lily_s_towards_s_sad_s_talk_displeased_] ...{i}okaayyy {/i} ...supongo que no necesito presentarte a Valerie ..."

# game/code/character_events/lily/lily_introduction.rpy:244 #-#-# translate crowdin lily_introduction_5571be57:
msgid "[_danny_s_towards_s_rise_s_talk_consternation_] Nah, we're obviously well acquainted."
msgstr "[_danny_s_towards_s_rise_s_talk_consternation_] Nah, obviamente estamos bien familiarizados."

# game/code/character_events/lily/lily_introduction.rpy:247 #-#-# translate crowdin lily_introduction_38b3342d:
msgid "[_danny_s_towards_s_idle_s_talk_happy_] So, anytime I can start?"
