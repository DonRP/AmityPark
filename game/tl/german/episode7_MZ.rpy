    # kate "{i}*Mwha*{/i}"
    kate "{i}*Mwha*{/i}"

# game/code/episodes/episode_7/episode7_MZ.rpy:1170
translate german A_Nightmare_Called_Ember_scene_8_Park_Evening_b77f403e:

