# game/code/episodes/episode_7/episode7_MZ.rpy:1211
translate german A_Nightmare_Called_Ember_scene_9_Mall_Afternoon_b486d699:

    # danny towards rise talk_amused "You just look... {i}different!{/i}"
    danny towards rise talk_amused "Du schaust gerade an... {i}anders!{/i}"

