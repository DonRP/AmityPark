# game/code/episodes/episode_8/episode8_MZ.rpy:11245 #-#-# translate crowdin Episode_8_stage_385_bedroom_night_bringing_closure_ae242d0e:
msgid "[_danny_] \"I want you to know that I tried writing to you while you were over seas, but perhaps the letters were lost along the way...\""
