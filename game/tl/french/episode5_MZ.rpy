    danny rise_one talk_happy_2 "Eh bien, qu'en est-il de la crème glacée ?"

# game/code/episodes/episode_5/episode5_MZ.rpy:3101
translate french A_Stroke_After_Midnight_scene_6b_bedroom_a7d0bf35:

    # desiree neutral_3 talk_frown towards "I'm afraid I don't know it."
    desiree neutral_3 talk_frown towards "J'ai bien peur que je ne le sache pas."

# game/code/episodes/episode_5/episode5_MZ.rpy:3103
translate french A_Stroke_After_Midnight_scene_6b_bedroom_e0d8aaa9:

    # danny talk_happy sad blank_stare "Oh, you'll love it, it's great! C'mon, I know a great sundae place this way!"
    danny talk_happy sad blank_stare "Oh, vous allez adorer, c'est génial! C'mon, je connais un grand endroit de soleil de cette façon !"

# game/code/episodes/episode_5/episode5_MZ.rpy:3123
