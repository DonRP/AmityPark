# game/code/episodes/episode_4/episode_4_twinkle_twinkle.rpy:420 #-#-# translate crowdin twinkle_twinkle_little_star_scene_2_hallway_7a1ef713:
msgid "[_danny_s_at_danny_s_sad_2_s_talk_frown_2_] Uhm, well... I mean, I sort of know him through his parents, cause they're ghost hunters and all... I have to make sure they don't catch me, ya know?"
